    "def generate_grayscale_images(names, counts, move_types, tensor, max_in_tensor, input_dates):\n",
    "    feature_count = sum(count for count in counts)\n",
    "    \n",
    "    for i in range(len(input_dates) - 1):\n",
    "        tensor_day = tensor[i:i + 1, :, :]\n",
    "        new_image = np.zeros((32, 32))\n",
    "        for j in range(feature_count - 1):\n",
    "            for row in range(32):\n",
    "                for col in range(32):\n",
    "                    tensor_day[0][row][col][j] = (tensor_day[0][row][col][j] * 255) / max_in_tensor\n",
    "                    new_image[row][col] = (tensor_day[0][row][col][j] * 255) / max_in_tensor\n",
    "            # 피처별로 끝낸 것 → 이미지 만들어서 저장\n",
    "            img = Image.fromarray(new_image.astype('uint8'), 'L')\n",
    "            img.save('%d%d%d%d.png' %(i, j, row, col))\n",
    "    "
   ]
